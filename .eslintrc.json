{
  "root": true,
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": { "project": ["./tsconfig.json"] },
  "plugins": [
    "prettier",
      "@typescript-eslint"
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}